    "# combining the neighborhoods into one line separated by a comma.\n",
    "df_codes = df_codes.groupby(['Postal Code', 'Borough'])['Neighborhood'].apply(', '.join).reset_index()\n",
    "df_codes.columns = ['Postal Code', 'Borough', 'Neighborhood']\n",
    "df_codes.head(12)"
